<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cp.user">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/application_logo_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity"
            android:label="Login">
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name=".ProfileActivity"
            android:label="Profile"
            android:parentActivityName="com.example.cp.user.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.cp.user.MainActivity" />
        </activity>
        <activity android:name=".Order_List_Activity"
            android:label="Order"
            android:parentActivityName="com.example.cp.user.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.cp.user.MainActivity" />
        </activity>
        <activity android:name=".Cart_List_Activity"
            android:label="Cart"
            android:parentActivityName="com.example.cp.user.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.cp.user.MainActivity" />
        </activity>
        <activity android:name=".Setting_Activity"
            android:label="Setting"
            android:parentActivityName="com.example.cp.user.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.cp.user.MainActivity" />
        </activity>
        <activity android:name=".About_Activity"
            android:label="About" android:parentActivityName="com.example.cp.user.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.cp.user.MainActivity" />
        </activity>
        <activity android:name=".checksum"></activity>
        <activity
            android:name="com.paytm.pgsdk.PaytmPGActivity"
            android:configChanges="keyboardHidden|orientation|keyboard"
            android:screenOrientation="portrait" />
    </application>

</manifest>